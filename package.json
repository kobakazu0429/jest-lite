{
  "name": "jest-lite",
  "version": "1.0.0-alpha.4",
  "description": "Run Jest in the browser",
  "repository": {
    "type": "git",
    "url": "https://github.com/kvendrik/jest-lite.git"
  },
  "keywords": [
    "jest",
    "browser"
  ],
  "main": "dist/core.js",
  "types": "dist/core.d.ts",
  "scripts": {
    "dev": "webpack-dev-server",
    "build": "webpack",
    "build:readme": "node scripts/build-readme.js",
    "lint": "yarn lint:ts && yarn lint:tslint && yarn lint:eslint",
    "lint:ts": "tsc --noEmit",
    "lint:eslint": "eslint src/**/*.ts --ext .ts",
    "lint:tslint": "tslint -c tslint.json --project tsconfig.json 'src/**/*.ts'"
  },
  "author": "Koen Vendrik <k.vendrik@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@jest/types": "^24.8.0",
    "enzyme": "^3.10.0",
    "jest-circus": "22.1.4",
    "jest-matchers": "^20.0.3",
    "jest-mock": "^24.8.0"
  },
  "devDependencies": {
    "@kobakazu0429/eslint-config-typescript": "^2.0.1",
    "@types/enzyme": "^3.10.1",
    "@types/jest-matchers": "^20.0.2",
    "@typescript-eslint/parser": "^1.11.0",
    "eslint": "^7.22.0",
    "parcel-bundler": "^1.12.3",
    "ts-loader": "^6.0.4",
    "typescript": "^3.5.2",
    "webpack": "^4.35.2",
    "webpack-cli": "^3.3.5",
    "webpack-dev-server": "^3.7.2"
  },
  "files": [
    "dist"
  ]
}
