    "@kobakazu0429/eslint-config-typescript": "^2.0.1",
    "@types/enzyme": "^3.10.1",
    "eslint": "^7.22.0",
    "html-webpack-plugin": "^4.5.2",
    "ts-loader": "^8.0.18",
    "typescript": "^4.2.3",
